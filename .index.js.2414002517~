client.on("messageCreate", async message => {
  if (message.author.bot) return;
  if (!message.channel.guild) return;
  if (message.content.startsWith(prefix + 'Ø§Ù…Ø¨Ø§Ø¯')) {
    if (!message.member.roles.cache.has('1128281171395104808')) {
      return message.reply({ content: "** ğŸ˜• You don't have permissions **" });
    }
    if (!message.guild.me.permissions.has('ADMINISTRATOR')) {
      return message.reply({ content: `** ğŸ˜• I couldn't edit the channel permissions. Please check my permissions and role position. **` });
    }
    let args = message.content.split(' ');
    args.shift();
    let attach = message.attachments.first();
    let content = args.join(' ');

    if (!content && !attach) {
      return message.channel.send({ content: '** âŒ Please include a message or attach an image **' });
    }
    message.delete();

    const embed = new MessageEmbed()
      .setDescription(content)
      .setColor("#006600");
      
    if (attach) {
      embed.setImage(attach.url);
    }

    message.channel.send({ embeds: [embed] });
  }
});


